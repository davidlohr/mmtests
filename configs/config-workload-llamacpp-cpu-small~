export MMTESTS="llamacpp"

# List of monitors
export RUN_MONITOR=yes
export MONITORS_ALWAYS=
export MONITORS_GZIP="proc-vmstat mpstat"
export MONITORS_WITH_LATENCY="vmstat"
export MONITOR_UPDATE_FREQUENCY=10

# LLama.cpp benchmark - Small model for quick testing
export LLAMACPP_MODEL_SIZE=$MEMTOTAL_BYTES*1/20
export LLAMACPP_COMPUTE_BACKEND=cpu
export LLAMACPP_MIN_THREADS=1
export LLAMACPP_MAX_THREADS=$NUMCPUS
export LLAMACPP_BATCH_SIZE=256
export LLAMACPP_ITERATIONS=5
export LLAMACPP_PROMPT_TOKENS=256
export LLAMACPP_GEN_TOKENS=64
